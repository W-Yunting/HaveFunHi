
.content {
		display: flex;
		flex: 1;
		flex-direction: column;
		align-items: center;
		justify-content: center;
}
.logo {
		height: 200rpx;
		width: 200rpx;
		margin-top: 200rpx;
		margin-left: auto;
		margin-right: auto;
		margin-bottom: 50rpx;
}
.swiper {
			width: 100%;
			height: 300rpx;
}
.swiper-item {
			display: block;
			height: 300rpx;
			line-height: 300rpx;
			text-align: center;
}
.swiper-list {
			margin-top: 40rpx;
			margin-bottom: 0;
}
.uni-common-mt {
			margin-top: 60rpx;
			position: relative;
}
.swiper-image {
			width: 100%;
			height: 300rpx;
			object-fit: cover;
}
.search-box {
			margin: 10px 0 ;
			display: flex;
			flex-direction: row;
			flex: 1;
			align-content: baseline;
}
.input {
			width: 500rpx;
			height: 28px;
			margin-right: 10rpx;
			border:1px solid #dcdfe6;
			border-radius: 6px;
}
.input-placeholder {
			margin-left: 10px;
			font-size: 12px;
}
